{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": [1, "always"],
        "space-before-function-paren": "off",
        "prefer-const": "off",
        "no-trailing-spaces": "off",
        "brace-style": "off",
        "no-func-assign": "off",
        "eqeqeq": "off",
        "no-extend-native": "warn",
        "eol-last":"off",
        "no-proto":"warn",
        "generator-star-spacing":"off"
    }
}